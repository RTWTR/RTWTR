<script>
    $(".ajax-call").on("submit", function (event) {
        event.preventDefault();

        var $this = $(this);


        var frmValues = $this.serialize();
        $.ajax({
            type: $this.attr("method"),
            url: $this.attr("action"),
            data: frmValues
        })
        .done(function () {
            
        })
        .fail(function () {
            
        });
    });
</script>